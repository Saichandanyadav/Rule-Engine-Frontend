{
    "version": 2,
    "builds": [
      {
        "src": "src/main.jsx",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "npm build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "env": {
      "API_URL": "https://rule-engine-backend-h2xv.onrender.com/api/rules"
    }
  }
  